package br.com.CursoModelo;

public class Curso {
	private int codigo;
	private int cargaHoraria;
	private int sala;
	private int quantiadeAlunos;
	private String nome;
	private String nomeProfessor;
	private Curso ArrayList;
	private double valorCurso;
	
	
	public Curso(int codigo, int cargaHoraria, int sala, int quantiadeAlunos, String nome, String nomeProfessor,
			Curso arrayList, double valorCurso) {
		super();
		this.codigo = codigo;
		this.cargaHoraria = cargaHoraria;
		this.sala = sala;
		this.quantiadeAlunos = quantiadeAlunos;
		this.nome = nome;
		this.nomeProfessor = nomeProfessor;
		ArrayList = arrayList;
		this.valorCurso = valorCurso;
	}


	public Curso() {
		super();
		// TODO Auto-generated constructor stub
	}


	public int getCodigo() {
		return codigo;
	}


	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}


	public int getCargaHoraria() {
		return cargaHoraria;
	}


	public void setCargaHoraria(int cargaHoraria) {
		this.cargaHoraria = cargaHoraria;
	}


	public int getSala() {
		return sala;
	}


	public void setSala(int sala) {
		this.sala = sala;
	}


	public int getQuantiadeAlunos() {
		return quantiadeAlunos;
	}


	public void setQuantiadeAlunos(int quantiadeAlunos) {
		this.quantiadeAlunos = quantiadeAlunos;
	}


	public String getNome() {
		return nome;
	}


	public void setNome(String nome) {
		this.nome = nome;
	}


	public String getNomeProfessor() {
		return nomeProfessor;
	}


	public void setNomeProfessor(String nomeProfessor) {
		this.nomeProfessor = nomeProfessor;
	}


	public Curso getArrayList() {
		return ArrayList;
	}


	public void setArrayList(Curso arrayList) {
		ArrayList = arrayList;
	}


	public double getValorCurso() {
		return valorCurso;
	}


	public void setValorCurso(double valorCurso) {
		this.valorCurso = valorCurso;
	}
	
	
	
	

	

}
