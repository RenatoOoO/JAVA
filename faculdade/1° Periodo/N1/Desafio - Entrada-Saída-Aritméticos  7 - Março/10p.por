programa
{
	inclua biblioteca Matematica --> mat
	funcao inicio()
	{
	real x, y, eq
	
	escreva ("Digite o Valor de X: ")
	leia (x)
	escreva ("Digite o valor de Y: ")
	leia(y)

	eq = (2*((mat.raiz(3*x + 30, 2.0))/3))+ (mat.raiz(y-32, 4.0))
	}
}
