package br.com.Ex_03;

public class Pessoa {

	private String nome;
	private String endereço;
	private int numero;
	
	public Pessoa (String nome, String endereço, int numero) {
		
		this.nome = nome;
		this.endereço = endereço;
		this.numero = numero;
	}
	
	public Pessoa(){
		
		
	}
	
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getEndereço() {
		return endereço;
	}
	public void setEndereço(String endereço) {
		this.endereço = endereço;
	}
	public int getNumero() {
		return numero;
	}
	public void setNumero(int numero) {
		this.numero = numero;
	}
	
	
	public String consultaEndereço() {
		
		return endereço;
		
	}
	
	public String consultaNome() {
		
		return nome;
	}
	
	
	
}
