package br.com.Ex_03;

import javax.swing.JOptionPane;

public class Compra {

	public static void main(String[] args) {
		
		Pessoa p1 = new Pessoa();
		Produto moto = new Produto();
		Endereco endereco = new Endereco ("logradouro", "cidade", "estado");
		
		p1.setNome( JOptionPane.showInputDialog("Digite seu nome: "));
		p1.setNumero(JOptionPane.showInputDialog("Digite seu Numero de inscrição: "));
		endereco.setLogradouro(JOptionPane.showInputDialog("Digite seu Edereço: "));
		endereco.setCidade(JOptionPane.showInputDialog("Digite a cidade onde vc mora: "));
		endereco.setEstado(JOptionPane.showInputDialog("Digite o seu Estado: "));
		
		JOptionPane.showMessageDialog(null, p1.toString());
	}

}
