	}	
	public Funcionario (double venda){
		this.venda= venda;
	}
	public Funcionario (Double salario){
		this.salario = salario;	
	}	
	
	
	
	public double getVenda(){
		return venda;
	}
	public double setVenda(){
		return venda;
	}	
	public double getSalario(){
		return salario;
	}
	public double setSalario(){
		return salario;
	}
		
