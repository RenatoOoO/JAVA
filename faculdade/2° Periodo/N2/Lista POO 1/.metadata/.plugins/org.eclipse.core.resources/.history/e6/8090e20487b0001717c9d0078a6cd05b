package br.com.MetodoVariavel;

	
public class MetodoVariavel {

	private int a;
	private int b;
	private int c;
	private int d;	
	
	public MetodoVariavel(int a, int b, int c, int d) {
		
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
	}
	
	
	public int getA() {
		return a;
	}
	public int getB() {
		return b;
	}

	public int getC() {
		return c;
	}
	
	public int getD() {
		return d;
	}
	public void  A(int a, int b, int c){
		
	}
	public int B(int a, int b, int c){
		int soma;
		return  soma = a + b + c + d;
	  
	}
}