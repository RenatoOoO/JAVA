package br.com.Lista;

	import java.util.Scanner;

public class EX_31 {

	private static Scanner in;

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		in = new Scanner (System.in);
		
		int vetor [] = new int [10];
		int menor = 0, maior = 0;
		
		for(int i=0; i<vetor.length; i++){
			
			System.out.printf("Digite o [%d]º de um numero: ", i+1);	
			vetor[i] = in.nextInt();
		}
		for(int i=0; i<vetor.length; i++){
			
			if(vetor[i] >maior){
				maior = vetor[i];
			}else if(vetor[i]<menor){
				menor = vetor[i];
			}
			
		System.out.println("O Menor elemento de X é:"+menor);	
		System.out.println("O Maior elemento de y é:"+maior);
		}
		
	}

}
